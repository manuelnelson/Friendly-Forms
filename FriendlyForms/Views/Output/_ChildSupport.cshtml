<script id="friendly-childSupportWorksheet-template" type="text/x-jquery-tmpl">
    <div class="row-fluid" style="text-align: center;">
        <h2>CHILD SUPPORT WORKSHEET</h2>
        <h2>Dekalb County Superior Court</h2>
    </div>
    <table>
        <thead>
            <tr>
                <th>Child Name</th>
                <th>Date of Birth</th>
            </tr>
        </thead>
        <tbody id="children">
        </tbody>
    </table>

    <table>
        <thead>
            <tr>
                <th></th>
                <th>${Father}</th>
                <th>${Mother}</th>
                <th>Total</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1. Parent's Monthly Gross Income</td>
                <td class="currency">$${FatherCsw.GrossIncome}</td>
                <td class="currency">$${MotherCsw.GrossIncome}</td>
                <td class="currency">$${TotalCsw.GrossIncome}</td>
            </tr>
            <tr>
                <td>2. Monthly Adjusted Income</td>
                <td class="currency">$${FatherCsw.AdjustedIncome}</td>
                <td class="currency">$${MotherCsw.AdjustedIncome}</td>
                <td class="currency">$${TotalCsw.AdjustedIncome}</td>
            </tr>
            <tr>
                <td>3. Pro Rata Shares of Combined Income</td>
                <td class="currency">$${FatherCsw.CombinedIncome}</td>
                <td class="currency">$${MotherCsw.CombinedIncome}</td>
                <td class="currency">$${TotalCsw.CombinedIncome}</td>
            </tr>
            <tr>
                <td>4. Basic Child Support Obligation</td>
                <td class="currency">$${FatherCsw.SupportObligation}</td>
                <td class="currency">$${MotherCsw.SupportObligation}</td>
                <td class="currency">$${TotalCsw.SupportObligation}</td>
            </tr>
            <tr>
                <td>5. Pro rata shares of Basic Child Support Obligation</td>
                <td class="currency">$${FatherCsw.ProRataObligation}</td>
                <td class="currency">$${MotherCsw.ProRataObligation}</td>
                <td class="currency"></td>
            </tr>
            <tr>
                <td>6. Adjustment for Work Related Child Care and Health Insurance Expenses</td>
                <td class="currency">$${FatherCsw.WorkRelatedExpenses}</td>
                <td class="currency">$${MotherCsw.WorkRelatedExpenses}</td>
                <td class="currency"></td>
            </tr>
            <tr>
                <td>7. Adjusted Child Support Obligation</td>
                <td class="currency">$${FatherCsw.AdjustedObligation}</td>
                <td class="currency">$${MotherCsw.AdjustedObligation}</td>
                <td class="currency"></td>
            </tr>
            <tr>
                <td>8. Adjustment for Additional Expenses Paid</td>
                <td class="currency">$${FatherCsw.AdjustedExpensesPaid}</td>
                <td class="currency">$${MotherCsw.AdjustedExpensesPaid}</td>
                <td class="currency"></td>
            </tr>
            <tr>
                <td>9. Presumptive Amount of Child Support</td>
                <td class="currency">$${FatherCsw.PresumptiveAmount}</td>
                <td class="currency">$${MotherCsw.PresumptiveAmount}</td>
                <td class="currency"></td>
            </tr>
        </tbody>
    </table>
    <br />
    <table>
        <thead>
            <tr>
                <th></th>
                <th>${Father}</th>
                <th>${Mother}</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>10. Deviations from Presumptive Child Support Amount</td>
                <td class="currency">$${FatherCsw.DeviationsAmount}</td>
                <td class="currency">$${MotherCsw.DeviationsAmount}</td>
            </tr>
            <tr>
                <td>11. Subtotal after Deviations</td>
                <td class="currency">$${FatherCsw.Subtotal}</td>
                <td class="currency">$${MotherCsw.Subtotal}</td>
            </tr>
            <tr>
                <td>12. Social Security Payments</td>
                <td class="currency">$${FatherCsw.SocialSecurity}</td>
                <td class="currency">$${MotherCsw.SocialSecurity}</td>
            </tr>
            <tr>
                <td>13. Final Child Support Amount</td>
                <td class="currency">$${FatherCsw.FinalAmount}</td>
                <td class="currency">$${MotherCsw.FinalAmount}</td>
            </tr>
            <tr>
                <td></td>
                <td class="currency"></td>
                <td class="currency"></td>
            </tr>
            <tr>
                <td>14. Uninsured Health Expenses</td>
                <td class="currency">$${FatherCsw.UninsuredExpenses}</td>
                <td class="currency">$${MotherCsw.UninsuredExpenses}</td>
            </tr>
        </tbody>
    </table>
    <p>The following schedules are attached to this worksheet: ${ValidSchedules}<br/>
        The following schedules are NOT attached to this worksheet: ${InvalidSchedules}
    </p>
    <label class="checkbox inline ">
         <input class="inline valid" type="checkbox">State of Georgia, ex rel., Department of Human Services, o/b/o (Check box only if action involves the Office of Child Support Services)</label>

</script>
<script id="friendly-CSWChildren-template" type="text/x-jquery-tmpl">
    {{each Children}}
    <tr>
        <td>${$value.Name}</td>
        <td>${$value.DateOfBirth}</td>
    </tr>
    {{/each}}
</script>
